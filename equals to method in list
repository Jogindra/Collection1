package com.infinite.collection.list;

import java.util.ArrayList;

public class List8equals {
	public static void main(String[] args) {
		ArrayList<Integer> a=new ArrayList <Integer>();
		a.add(1);
		a.add(2);
		a.add(3);
		a.add(4);
		a.add(5);
		System.out.println("The elements of the first List"+a);
		
		ArrayList<Integer> a1=new ArrayList <Integer>();
		a1.add(1);
		a1.add(2);
		a1.add(3);
		a1.add(4);
		a1.add(6);
		System.out.println("The elements of the 2nd List"+a1);
		System.out.println(a.get(4));
		System.out.println(a1.get(4));
		
   boolean b=a.equals(a1);
   //if the contents of the two list are equals the return true otherwise false...
   if(b)
	   System.out.println(b);
   else
	   System.out.println(b);
		
	}

}
